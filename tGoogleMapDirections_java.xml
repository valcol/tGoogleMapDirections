<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.01" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="COLMANT Valentin" RELEASE_DATE="20150205"
		STARTABLE="false" DATA_AUTO_PROPAGATE="false">
		<SIGNATURE></SIGNATURE>
	</HEADER>
	<FAMILIES>
		<FAMILY>Misc</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL>https://developers.google.com/maps/documentation/directions/</URL>
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1"
			MIN_OUTPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="LOOKUP" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="1">
			<TABLE READONLY="true">
				<COLUMN NAME="DISTANCE" TYPE="id_Double" LENGTH="10" READONLY="false"
					CUSTOM="true" />
				<COLUMN NAME="DISTANCE_TEXT" TYPE="id_String" LENGTH="100" READONLY="false"
					CUSTOM="true" />	
				<COLUMN NAME="DURATION" TYPE="id_Double" LENGTH="10" READONLY="false"
					CUSTOM="true" />	
				<COLUMN NAME="DURATION_TEXT" TYPE="id_String" LENGTH="100" READONLY="false"
					CUSTOM="true" />
				<COLUMN NAME="END_ADRESS" TYPE="id_String" LENGTH="100" READONLY="false"
					CUSTOM="true" />	
				<COLUMN NAME="END_ADRESS_LAT" TYPE="id_Double" LENGTH="20" READONLY="false"
					CUSTOM="true" />	
				<COLUMN NAME="END_ADRESS_LNG" TYPE="id_Double" LENGTH="20" READONLY="false"
					CUSTOM="true" />	
				<COLUMN NAME="START_ADRESS" TYPE="id_String" LENGTH="100" READONLY="false"
					CUSTOM="true" />
				<COLUMN NAME="START_ADRESS_LAT" TYPE="id_Double" LENGTH="20" READONLY="false"
					CUSTOM="true" />
				<COLUMN NAME="START_ADRESS_LNG" TYPE="id_Double" LENGTH="20" READONLY="false"
					CUSTOM="true" />
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="DIRECTIONS_FROM" FIELD="PREV_COLUMN_LIST"
			REQUIRED="false" NUM_ROW="2">
		</PARAMETER>
		<PARAMETER NAME="DIRECTIONS_TO" FIELD="PREV_COLUMN_LIST"
			REQUIRED="false" NUM_ROW="3">
		</PARAMETER>
		<PARAMETER NAME="GOOGLE_APP_ID" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="4">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="LANG" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="5">
			<DEFAULT>"fr"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="MODE" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="6">
			<DEFAULT>"driving"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="AVOID" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="7">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="UNITS" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="8">
			<DEFAULT>"metric"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DEPARTURE_TIME" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="9">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ARRIVAL_TIME" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="9">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TRANSIT_MODE" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="10">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TRANSIT_ROUTING_PREFERENCES" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="11">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="tGoogleMapDirections" MODULE="tGoogleMapDirections.jar"
				REQUIRED="true" />
			<IMPORT NAME="JSON" MODULE="json-20090211.jar"
				REQUIRED="true" />
			
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>